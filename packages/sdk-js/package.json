{
  "name": "@gooddollar/goodcollective-sdk",
  "license": "MIT",
  "version": "1.3.2",
  "main": "./dist/sdk",
  "types": "./types/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "rm -fr dist types && tsc && vite build",
    "test:setup": "yarn exec ./scripts/deployContracts.sh",
    "test": "vitest run",
    "watch": "vitest",
    "bump": "yarn version patch && yarn build && git add package.json && git commit -m \"version bump\"",
    "publish": "yarn build && npm publish --access public",
    "lint": "eslint .",
    "format": "prettier -w ./src ",
    "format:check": "prettier -c ./src"
  },
  "files": [
    "src",
    "dist",
    "types",
    "package.json"
  ],
  "devDependencies": {
    "@types/eslint": "^8.40.2",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "ethers": "5",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.2",
    "typescript": "5",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  },
  "peerDependencies": {
    "ethers": "^5.*"
  },
  "dependencies": {
    "@gooddollar/goodcollective-contracts": "^1.2.0",
    "@superfluid-finance/sdk-core": "^0.6.12",
    "@web3-storage/w3up-client": "^15.0.0",
    "ethereum-multicall": "^2.24.0",
    "graphql": "^16.7.1"
  },
  "packageManager": "yarn@3.6.0"
}
